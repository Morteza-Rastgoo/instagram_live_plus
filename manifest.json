{
  "name": "live plus",
  "description": "fullscreen landscape Instagram live",
  "version": "0.1.0",
  "author": "Esmaeil Bahrani Fard",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "instagram live plus"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
